<html>
  <head>
    <!-- AFTER HTML loads in browser, browser will request for CSS. -->
    <link rel="stylesheet" href="styles.css" />
  </head>
  <body>
    <% for (let i = 0; i < years.length; i+= 1) { %>
    <ul>
      <li>
        <% const year = years[i] %> <% const link = `/year-sighting/${year}` %>
        <a href="<%= link %>"> <%= year %> </a>
      </li>
    </ul>
    <% } %>
  </body>
</html>
